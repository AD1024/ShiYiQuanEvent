<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_fav_event_search"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingEnd="5sp"
    android:paddingLeft="5sp"
    tools:context="ccoderad.bnds.shiyiquanevent.activities.FavEventSearchResultActivity">

    <ListView
        android:id="@+id/fav_event_search_result_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>
    <TextView
        android:id="@+id/fav_event_search_result_no_result_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/search_result_null"
        android:textSize="25sp"
        android:visibility="gone"
        android:layout_centerInParent="true"
        />

</RelativeLayout>
